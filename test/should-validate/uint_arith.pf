import UInt

assert 1 + 2 + 3 = 3 + 2 + 1

theorem T1: all x:UInt. x + 3 + 4 = 5 + 2 + x
proof
  arbitrary x:UInt
  equations
      x + 3 + 4
        = x + 7 by .
    ... = 7 + x by uint_add_commute[x, 7]
    ... = 5 + 2 + x by .
end

