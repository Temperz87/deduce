import List 
import UInt

theorem length_drop2 : <T> all xs : List<T>, n : UInt.
  length(drop(xs, n)) = length(xs) âˆ¸ n
proof
  arbitrary T:type
  induction List<T>
  case empty {
    arbitrary n : UInt
    expand drop
    // Make sure TermInst is explicit around the empty list.
    ?
  }
  case node(x, xs') assume IH {
    arbitrary n : UInt
    expand drop
    ?
  }
end
