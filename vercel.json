{
    "version": 2,
    "builds": [
        {
            "src": "live_code_vercel_api/api.py",
            "use": "@vercel/python",
			"config": {
				"buildCommand": "pip install -r live_code_vercel_api/requirements.txt && mv lib /tmp/lib"
			}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "live_code_vercel_api/api.py"
        }
    ]
}
